class Person:
    def __init__(self, name):
        self.name = name

    def show(self):
        print("Name:", self.name)

class Student(Person):
    def __init__(self, name, branch):
        super().__init__(name)
        self.branch = branch

    def display(self):
        self.show()
        print("Branch:", self.branch)

s = Student("Spoorthi", "AI")
s.display()
